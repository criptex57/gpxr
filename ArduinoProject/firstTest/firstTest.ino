#include <OLED_I2C.h>
OLED  myOLED(SDA, SCL, 8);
extern uint8_t SmallFont[];
extern uint8_t BigNumbers[];

void setup()
{
  myOLED.begin();
  myOLED.setFont(SmallFont);
   Serial.begin(9600);
}

void loop()
{
  while (Serial.available()) {
    myOLED.clrScr();

    String input = Serial.readString();
    int a, b, c, d, e;
    
    for (int i = 0; i < input.length(); i++) {
      if (input.substring(i, i+1) == "$") {
        a = i;
      }
      if (input.substring(i, i+1) == "#") {
        b = i;
      }
      if (input.substring(i, i+1) == "@") {
        c = i;
      }
      if (input.substring(i, i+1) == "!") {
        d = i;
      }
      if (input.substring(i, i+1) == "&") {
        e = i;
      }
    }

    
    myOLED.setFont(BigNumbers);
      myOLED.printNumI(input.substring(a+1, b).toInt(), 0, 0); //turn distance
    
     int dtnp = input.substring(d+1, e).toInt(); //directionToNearestPoint
      int side = input.substring(c+1, d).toInt();
    int type = input.substring(b+1, c).toInt();
     
    if(dtnp > 337 || dtnp < 23){ //aa 0  337-23 bb 45 23-68 cc 90 68-113 dd 135 113-158 ee 180 158-203 ff 225 203-248 gg 270 248-293 qq 315 293-337
      myOLED.drawBitmap(0, 32, aa, 32, 32);
    }
    else if(dtnp >= 23 && dtnp < 68){
      myOLED.drawBitmap(0, 32, bb, 32, 32);
    }
    else if(dtnp >= 68 && dtnp < 113){
      myOLED.drawBitmap(0, 32, cc, 32, 32);
    }
    else if(dtnp >= 113 && dtnp < 158){
      myOLED.drawBitmap(0, 32, dd, 32, 32);
    }
    else if(dtnp >= 158 && dtnp < 203){
      myOLED.drawBitmap(0, 32, ee, 32, 32);
    }
    else if(dtnp >= 203 && dtnp < 248){
      myOLED.drawBitmap(0, 32, ff, 32, 32);
    }
    else if(dtnp >= 248 && dtnp < 293){
      myOLED.drawBitmap(0, 32, gg, 32, 32);
    }
    else if(dtnp >= 293 && dtnp < 337){
      myOLED.drawBitmap(0, 32, qq, 32, 32);
    }


    if(type == 1 && side == 2){
      myOLED.drawBitmap(64, 0, hright, 64, 64);
    }
    else if(type == 2 && side == 2){
      myOLED.drawBitmap(64, 0, right, 64, 64);
    }
    else if(type == 3 && side == 2){
      myOLED.drawBitmap(64, 0, lright, 64, 64);
    }
    else if(type == 1 && side == 1){
      myOLED.drawBitmap(64, 0, hleft, 64, 64);
    }
    else if(type == 2 && side == 1){
      myOLED.drawBitmap(64, 0, left , 64, 64);
    }
    else if(type == 3 && side == 1){
      myOLED.drawBitmap(64, 0, lleft, 64, 64);
    }
    
    myOLED.update();
  }
}


